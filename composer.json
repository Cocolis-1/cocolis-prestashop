{
  "name": "cocolis/cocolisprestashop",
  "description": "Utilisez cocolis.fr comme mode de livraison. Spécialisé dans la livraison communautaire, Cocolis vous permettra d'envoyer des colis hors format.",
  "authors": [
      {
          "name": "Cocolis",
          "email": "contact@cocolis.fr"
      }
  ],
  "require": {
    "php": ">=5.6.0",
    "cocolis/php": "dev-master",
    "guzzlehttp/guzzle": "^6.5"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "PrestaShop\\Module\\Cocolis\\": "src/",
      "Cocolis\\Api\\" : "vendor/cocolis/php/src/"
    },
    "config": {
      "prepend-autoloader": false
    },
    "type": "prestashop-module"
  },
  "config": {
      "preferred-install": "dist",
      "prepend-autoloader": false
  },
  "type": "prestashop-module",
  "author": "Cocolis",
  "license": "MIT"
}
