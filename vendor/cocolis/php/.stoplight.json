{
  "formats": {
    "openapi": {
      "rootDir": "docs/apidoc",
      "include": ["docs/apidoc", "docs/apidoc/**"]
    },
    "markdown": {
      "rootDir": "docs",
      "include": ["docs", "docs/**"]
    },
    "image": {
      "rootDir": "docs/assets/images",
      "include": ["docs/assets/images", "docs/assets/**"]
    },
    "json_schema": {
      "rootDir": "docs/models",
      "include": ["docs/models", "docs/models/**"]
    }
  },
  "exclude": ["tests"]
}